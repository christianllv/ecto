{
  "name": "ecto",
  "version": "1.1.2",
  "description": "Modern Template Consolidation Engine for EJS, Markdown, Pug, Nunjucks, and Handlebars",
  "main": "./dist/ecto.js",
  "types": "./dist/ecto.d.ts",
  "repository": "https://github.com/jaredwray/ecto.git",
  "author": "Jared Wray <me@jaredwray.com>",
  "license": "MIT",
  "keywords": [
    "templates",
    "generator",
    "framework",
    "html",
    "handlebars",
    "hbs",
    "markdown",
    "hjs",
    "ejs",
    "consolidate",
    "consolidatejs",
    "pug",
    "jade",
    "nunjucks",
    "njk",
    "mustache",
    "liquid",
    "liquidjs"
  ],
  "scripts": {
    "watch": "tsc -w -p .",
    "clean": "rm -rf ./dist && rm -rf node_modules",
    "test": "jest --coverage",
    "compile": "tsc -p .",
    "build": "rm -rf ./dist && npm i && npm run test && npm run compile"
  },
  "dependencies": {
    "ejs": "^3.1.6",
    "fs-extra": "^10.0.0",
    "handlebars": "^4.7.7",
    "handlebars-helpers": "^0.10.0",
    "helper-date": "^1.0.1",
    "liquidjs": "^9.33.1",
    "marked": "^4.0.10",
    "nunjucks": "^3.2.3",
    "pug": "^3.0.2",
    "winston": "^3.4.0"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.0",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/handlebars-helpers": "^0.5.3",
    "@types/jest": "^27.4.0",
    "@types/marked": "^4.0.1",
    "@types/mustache": "^4.1.2",
    "@types/nunjucks": "^3.2.1",
    "@types/pug": "^2.0.6",
    "codecov": "^3.8.3",
    "jest": "^27.4.7",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.4"
  },
  "files": [
    "dist",
    "bin"
  ],
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      "/dist"
    ]
  }
}
