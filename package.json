{
  "name": "ecto",
  "version": "2.2.0",
  "description": "Modern Template Consolidation Engine for EJS, Markdown, Pug, Nunjucks, Liquid, and Handlebars",
  "main": "./dist/ecto.js",
  "types": "./dist/ecto.d.ts",
  "repository": "https://github.com/jaredwray/ecto.git",
  "author": "Jared Wray <me@jaredwray.com>",
  "license": "MIT",
  "keywords": [
    "templates",
    "generator",
    "framework",
    "html",
    "handlebars",
    "hbs",
    "markdown",
    "hjs",
    "ejs",
    "consolidate",
    "consolidatejs",
    "pug",
    "jade",
    "nunjucks",
    "njk",
    "mustache",
    "liquid",
    "liquidjs"
  ],
  "scripts": {
    "watch": "tsc -w -p .",
    "clean": "rm -rf ./dist && rm -rf node_modules",
    "test": "jest --coverage",
    "compile": "tsc -p .",
    "build": "rm -rf ./dist && npm i && npm run test && npm run compile",
    "website:build": "npx docula",
    "website:serve": "npx docula serve"
  },
  "dependencies": {
    "@jaredwray/fumanchu": "^1.0.1",
    "@markdoc/markdoc": "^0.3.2",
    "ejs": "^3.1.9",
    "fs-extra": "^11.1.1",
    "helper-date": "^1.0.1",
    "liquidjs": "^10.9.1",
    "nunjucks": "^3.2.4",
    "pug": "^3.0.2",
    "underscore": "^1.13.6",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.2",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.1",
    "@types/handlebars-helpers": "^0.5.3",
    "@types/jest": "^29.5.4",
    "@types/mustache": "^4.2.2",
    "@types/nunjucks": "^3.2.3",
    "@types/pug": "^2.0.6",
    "@types/underscore": "^1.11.8",
    "docula": "^0.3.4",
    "jest": "^29.6.4",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "files": [
    "dist",
    "bin"
  ],
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      "/dist"
    ]
  }
}
