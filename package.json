{
  "name": "ecto",
  "version": "2.2.2",
  "description": "Modern Template Consolidation Engine for EJS, Markdown, Pug, Nunjucks, Liquid, and Handlebars",
  "main": "./dist/ecto.js",
  "types": "./dist/ecto.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaredwray/ecto.git"
  },
  "author": "Jared Wray <me@jaredwray.com>",
  "license": "MIT",
  "keywords": [
    "templates",
    "generator",
    "framework",
    "html",
    "handlebars",
    "hbs",
    "markdown",
    "hjs",
    "ejs",
    "consolidate",
    "consolidatejs",
    "pug",
    "jade",
    "nunjucks",
    "njk",
    "mustache",
    "liquid",
    "liquidjs"
  ],
  "scripts": {
    "watch": "tsc -w -p .",
    "clean": "rimraf ./dist && rimraf yarn.lock && rimraf package-lock.json && rimraf ./coverage && rimraf ./node_modules",
    "test": "xo --fix && vitest run --coverage",
    "compile": "tsc -p .",
    "build": "rimraf ./dist && yarn && yarn test && yarn compile",
    "website:build": "npx docula",
    "website:serve": "npx docula serve"
  },
  "dependencies": {
    "@jaredwray/fumanchu": "^1.0.3",
    "@markdoc/markdoc": "^0.3.2",
    "ejs": "^3.1.9",
    "fs-extra": "^11.1.1",
    "helper-date": "^1.0.1",
    "liquidjs": "^10.9.2",
    "nunjucks": "^3.2.4",
    "pug": "^3.0.2",
    "underscore": "^1.13.6",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.3",
    "@types/express": "^4.17.18",
    "@types/fs-extra": "^11.0.2",
    "@types/mustache": "^4.2.3",
    "@types/nunjucks": "^3.2.4",
    "@types/pug": "^2.0.7",
    "@types/underscore": "^1.11.9",
    "@vitest/coverage-v8": "^0.34.6",
    "docula": "^0.3.6",
    "rimraf": "^5.0.5",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6",
    "webpack": "^5.89.0",
    "xo": "^0.56.0"
  },
  "files": [
    "dist",
    "bin"
  ]
}
